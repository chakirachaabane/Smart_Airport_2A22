QTabBar::tab {
    color: #0C5A23;
    border: 1px solid #08485E;
    border-bottom-style: none;
    background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:1 #1BEE9A, stop:.4 #1BEE9A);
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 3px;
    padding-bottom: 2px;
    margin-right: -1px;
    border-top-right-radius:3px;
}

QTabWidget::pane {
    border: 3px solid #7CD5F3;
    top: 1px;
    
}

QTabBar::tab:last
{
    margin-right: 0; /* the last selected tab has nothing to overlap with on the right */
    border-top-right-radius: 10px;
}

QTabBar::tab:first:!selected
{
 margin-left: 0px; /* the last selected tab has nothing to overlap with on the right */


    border-top-left-radius: 3px;
}

QTabBar::tab:!selected
{
    color:#08485E;
    border-bottom-style: solid;
    margin-top: 3px;
    background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:1 #ADFFBC, stop:.4 #21C036);
}
QTabBar::tab:selected
{
     border-top-right-radius: 10px;

